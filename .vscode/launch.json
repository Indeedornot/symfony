{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Listen for Xdebug",
            "type": "php",
            "request": "launch",
            "port": 443,
            "hostname": "127.0.0.1",
            "pathMappings": {
              "/app": "${workspaceFolder}"
            },
            "stopOnEntry": true,
            "proxy": {
              "enable": true,
              "port": 9100,
              "host": "0.0.0.0",
              "key": "indeedornot",
              "timeout": 6000
            }
        }
    ]
}
